<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity">

    <TextView
        android:id="@+id/ageView"
        android:layout_width="108dp"
        android:layout_height="56dp"
        android:layout_marginStart="22dp"
        android:layout_marginLeft="22dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:autoText="false"
        android:text="Age:"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toTopOf="@+id/editProfileBtn"
        app:layout_constraintEnd_toStartOf="@+id/ageView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailView" />

    <TextView
        android:id="@+id/fullnameView"
        android:layout_width="109dp"
        android:layout_height="55dp"
        android:layout_marginStart="22dp"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="106dp"
        android:layout_marginBottom="13dp"
        android:autoText="false"
        android:text="FullName:"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toTopOf="@+id/emailView"
        app:layout_constraintEnd_toStartOf="@+id/fullnameView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/greeting" />

    <TextView
        android:id="@+id/emailView"
        android:layout_width="108dp"
        android:layout_height="55dp"
        android:layout_marginStart="22dp"
        android:layout_marginLeft="22dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:layout_marginBottom="14dp"
        android:autoText="false"
        android:text="Email:"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toTopOf="@+id/ageView"
        app:layout_constraintEnd_toStartOf="@+id/emailView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fullnameView" />

    <Button
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="28dp"
        android:text="Logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ageView2" />

    <TextView
        android:id="@+id/greeting"
        android:layout_width="301dp"
        android:layout_height="0dp"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="55dp"
        android:layout_marginRight="55dp"
        android:layout_marginBottom="174dp"
        android:autoText="true"
        android:background="#00EA6F7E"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@+id/emailView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/fullnameView2"
        android:layout_width="250dp"
        android:layout_height="56dp"
        android:layout_marginTop="106dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="12dp"
        android:autoText="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toTopOf="@+id/emailView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/greeting" />

    <TextView
        android:id="@+id/ageView2"
        android:layout_width="250dp"
        android:layout_height="0dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="284dp"
        android:autoText="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toTopOf="@+id/logout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/emailView2"
        app:layout_constraintTop_toBottomOf="@+id/emailView2" />

    <TextView
        android:id="@+id/emailView2"
        android:layout_width="250dp"
        android:layout_height="0dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="13dp"
        android:autoText="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toTopOf="@+id/ageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/fullnameView2"
        app:layout_constraintTop_toBottomOf="@+id/greeting" />

    <Button
        android:id="@+id/editProfileBtn"
        android:layout_width="194dp"
        android:layout_height="0dp"
        android:layout_marginBottom="254dp"
        android:text="edit profile"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ageView" />

</androidx.constraintlayout.widget.ConstraintLayout>